CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in[0]=indata[0], in[1]=indata[1], in[2]=indata[2], in[3]=indata[3], in[4..15]=false, load=load, out=reg_out);

    Counter(increment=enable, reset=load, out=count[0..1]);

    Mux4Way(a=reg_out[0], b=reg_out[1], c=reg_out[2], d=reg_out[3],sel=count, out=sout);
    And(a=count[0], b=count[1], out=count_is_3);
    And(a=enable, b=count_is_3, out=complete);
}
